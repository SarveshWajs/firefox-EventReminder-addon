{
    "name": "personalized-Events",
    "version": "0.0.4",
    "description": "Create personalized Events",
    "manifest_version": 2,
    "icons": {
        "16": "assets/images/planner16.png",
        "32": "assets/images/planner32.png",
        "48": "assets/images/planner64.png",
        "128": "assets/images/planner128.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "browser_style": true
    },
    "permissions": [
        "storage",
        "alarms",
        "notifications"
    ],
    "options_ui": {
        "page": "options.html",
        "browser_style": false
    },
    "background": {
        "scripts": ["assets/js/background.js"]
    },
    "browser_specific_settings": {
  "gecko": {
    "id": "addon@example.com",
    "strict_min_version": "42.0"
  }
}
}